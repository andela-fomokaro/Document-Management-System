  "main": "webpack.config.js",
  "scripts": {
    "test": "npm-run-all migrate:test && NODE_ENV=test mocha --compilers js:babel-core/register ./server/test/**/*.js --timeout 1000000",
    "cover": "NODE_ENV=test node_modules/babel-cli/bin/babel-node.js node_modules/babel-istanbul/lib/cli.js cover node_modules/mocha/bin/_mocha -- server/test/**/*.js",
    "coveralls": "npm run 'migrate:test' && npm run cover && cat ./coverage/lcov.info | coveralls ",
    "migrate:test:undo": "NODE_ENV=test sequelize db:migrate:undo:all",
    "migrate:test": "NODE_ENV=test sequelize db:migrate",
    "start": "babel-node server.js",
    "test-e2e": "nightwatch",
    "test:frontend": "jest",
    "test:client": "mocha ./client/tests/**/**/*.spec.js --compilers js:babel-register",
    "test:clientActions": "mocha ./client/tests/actions/**/*.spec.js --compilers js:babel-register",
    "test:watch": "npm test:frontend -- --watch",
    "start:dev": "babel-watch ../server/config/app.js",
    "e2e-setup": "selenium-standalone install"
  },
  "jest": {
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "modulePaths": "<rootDir>/client",
    "roots": "client/",
    "unmockedModulePathPatterns": "<rootDir>/node_modules"
  },
  "repository": {
    "type": "git",
